{
    "manifest_version" : 1,
    "name" : "Plipeo Search Engine",
    "description" : "Smart, safe and kid-friendly search engine designed to provide the best search results.",
    "version" : "1.0.0",
    "icons": {
        "48": "plipeo-icon-fox.svg",
        "96": "plipeo-icon-fox.svg"
    },
    "browser_specific_settings" : {
       "gecko" : {
          "strict_min_version" : "65.0"
       }
    },
    "chrome_settings_overrides" : {
       "search_provider" : {
          "encoding" : "UTF-8",
          "favicon_url" : "https://plipeo.com/assets/img/ico/apple-icon-180x180.png",
          "is_default" : yes,
          "keyword" : "=",
          "name" : "Plipeo",
          "search_url" : "https://plipeo.com/?q={searchTerms}"
       }
    }
 }


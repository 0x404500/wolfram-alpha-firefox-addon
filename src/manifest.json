{
    "manifest_version" : 2,
    "name" : "000 Search Engine",
    "description" : "Adds search engine that asks 000.com",
    "version" : "1.1.0",
    "icons": {
        "48": "icon.svg",
        "96": "icon.svg"
    },
    "browser_specific_settings" : {
       "gecko" : {
          "strict_min_version" : "65.0"
       }
    },
    "chrome_settings_overrides" : {
       "search_provider" : {
          "encoding" : "UTF-8",
          "favicon_url" : "https://000.web.id/assets/img/favicon.png",
          "is_default" : false,
          "keyword" : "=",
          "name" : "000|Alpha",
          "search_url" : "https://000.web.id/?q={searchTerms}"
       }
    }
 }

